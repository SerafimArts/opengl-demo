<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd"
>
    <paths>
        <output>storage/phpdoc</output>
        <cache>storage/phpdoc-cache</cache>
    </paths>
    <version number="3.0.0">
        <api>
            <default-package-name>BicEngine</default-package-name>
            <source dsn=".">
                <path>libs</path>
            </source>
        </api>
    </version>

    <template>
        <parameter name="positive-int" value="int" />
    </template>
</phpdocumentor>