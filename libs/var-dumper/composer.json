{
    "name": "bic-engine/var-dumper",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/var-dumper": "^5.4|^6.0"
    },
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ],
        "psr-4": {
            "Bic\\VarDumper\\": "src"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.24",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
