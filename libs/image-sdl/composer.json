{
    "name": "bic-engine/image-sdl",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-ffi": "*",
        "ffi/env": "^1.0",
        "ffi/proxy": "^1.0",
        "ffi/location": "^1.0",
        "bic-engine/image": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bic\\Image\\SDL\\": "src"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4|^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Bic\\Image\\SDL\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
