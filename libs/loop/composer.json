{
    "name": "bic-engine/loop",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "react/promise": "*"
    },
    "autoload": {
        "psr-4": {
            "Bic\\Loop\\": "src"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^5.4|^6.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.24",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Bic\\Loop\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
