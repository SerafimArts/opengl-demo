{
    "name": "bic-engine/ui-sdl2",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bic-engine/dispatcher": "^1.0",
        "bic-engine/ui": "^1.0",
        "bic-engine/lib-sdl2": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bic\\UI\\SDL2\\": "src"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^5.4|^6.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.24"
    },
    "autoload-dev": {
        "psr-4": {
            "Bic\\UI\\SDL2\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "optimize-autoloader": true,
        "secure-http": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
