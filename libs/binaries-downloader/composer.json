{
    "name": "local/binaries-downloader",
    "type": "composer-plugin",
    "require": {
        "php": "^8.1",
        "composer-plugin-api": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Local\\BinariesDownloader\\": "src"
        }
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "symfony/var-dumper": "^5.4|^6.0"
    },
    "extra": {
        "class": "Local\\BinariesDownloader\\Plugin",
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
